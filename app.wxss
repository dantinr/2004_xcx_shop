/* iconfont 开始 */
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_2216067_vypx6fr9btk.eot?t=1606013329698'); /* IE9 */
  src: url('//at.alicdn.com/t/font_2216067_vypx6fr9btk.eot?t=1606013329698#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_2216067_vypx6fr9btk.woff?t=1606013329698') format('woff'),
  url('//at.alicdn.com/t/font_2216067_vypx6fr9btk.ttf?t=1606013329698') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_2216067_vypx6fr9btk.svg?t=1606013329698#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconarrow:before {
  content: "\e610";
}

.iconphone:before {
  content: "\e634";
}

.iconshare:before {
  content: "\e64c";
}

.iconcart:before {
  content: "\e60b";
}

.iconhome:before {
  content: "\e652";
}


/* iconfont 结束*/



